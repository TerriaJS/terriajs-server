{
  "name": "terriajs-server",
  "version": "3.3.1-alpha.2",
  "description": "NodeJS server for TerriaJS, consisting of a CORS proxy, proj4 CRS lookup service, ogr2ogr conversion service, and express static server.",
  "engineStrict": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/app.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint lib/**/*.ts lib/**/*.js spec/**/*.ts",
    "build": "tsc --module commonjs ",
    "build:watch": "tsc -w --incremental",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "test": "ts-node node_modules/.bin/jasmine",
    "start": "pm2 start ecosystem.config.js --update-env --",
    "stop": "pm2 stop ecosystem.config.js",
    "watch": "pm2 start ecosystem-watch.config.js --update-env --",
    "restart": "pm2 restart ecosystem.config.js --update-env --",
    "postinstall": "npm run build",
    "postpublish": "bash -c \"git tag -a ${npm_package_version} -m \"${npm_package_version}\" && git push origin ${npm_package_version}\""
  },
  "bin": {
    "terriajs-server": "./terriajs-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerriaJS/TerriaJS-Server"
  },
  "keywords": [
    "terriajs",
    "National Map"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TerriaJS/TerriaJS-Server/issues"
  },
  "homepage": "https://github.com/TerriaJS/TerriaJS-Server",
  "dependencies": {
    "aws-sdk": "^2.456.0",
    "base-x": "^3.0.5",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.15.0",
    "catalog-converter": "0.0.2-alpha.2",
    "compression": "^1.6.0",
    "cors": "^2.7.1",
    "express": "^4.8.0",
    "express-brute": "^1.0.1",
    "formidable": "^1.0.17",
    "json5": "^2.1.0",
    "morgan": "^1.7.0",
    "pm2": "^3.2.2",
    "proj4": "^2.3.12",
    "proj4js-defs": "0.0.1",
    "range_check": "^1.4.0",
    "request": "^2.67.0",
    "request-promise": "^4.0.1",
    "terriajs-ogr2ogr": "^1.0.0",
    "when": "^3.7.7",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/jasmine": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "jasmine": "^3.4.0",
    "supertest": "^4.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.2"
  }
}
